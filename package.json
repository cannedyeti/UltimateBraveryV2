{
  "name": "LOL",
  "version": "1.1.0",
  "description": "League Of Legends API Sandbox",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run server\" \"PORT=3000 npm run client\"",
    "postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Connor Potebnya",
  "license": "NA",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "async": "^3.0.0",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "connect-history-api-fallback": "^1.6.0",
    "cors": "^2.8.5",
    "discord.js": "^11.5.1",
    "dotenv": "^8.0.0",
    "express": "^4.16.2",
    "http-proxy-middleware": "^0.19.1",
    "mysql": "^2.17.1",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1"
  }
}
